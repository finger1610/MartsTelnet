        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MartsTelnet"
        mc:Ignorable="d"
        Title="ShowList" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstboxIP" Margin="86,43,86,77" MouseDoubleClick="lstboxIP_MouseDoubleClick"/>
        <Button x:Name="btn" Content="OK" HorizontalAlignment="Center" Margin="0,362,0,0" VerticalAlignment="Top" Click="btn_Click" RenderTransformOrigin="0.85,-1.752" Height="41" Width="61"/>
    </Grid>
</Window>
